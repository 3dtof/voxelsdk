# - Config file for the FooBar package
# It defines the following variables
#  VOXEL_INCLUDE_DIRS - include directories for FooBar
#  VOXEL_LIBRARIES    - libraries to link against
 
# Compute paths
get_filename_component(VOXEL_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

get_filename_component(VOXEL_INCLUDE_DIRS "${VOXEL_CMAKE_DIR}" PATH)
get_filename_component(VOXEL_INCLUDE_DIRS "${VOXEL_INCLUDE_DIRS}" PATH)

set(VOXEL_INCLUDE_DIRS "${VOXEL_INCLUDE_DIRS}/include/voxel")
 
# Our library dependencies (contains definitions for IMPORTED targets)
include("${VOXEL_CMAKE_DIR}/VoxelTargets.cmake")
 
# These are IMPORTED targets created by FooBarTargets.cmake
set(VOXEL_LIBRARIES voxel)